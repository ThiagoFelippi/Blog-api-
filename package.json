    "@commitlint/config-angular": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/express": "^4.17.7",
    "@types/graphql": "^14.5.0",
    "@types/node": "^14.0.23",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
